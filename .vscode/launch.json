{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Java",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node.js",
      "program": "${file}",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
